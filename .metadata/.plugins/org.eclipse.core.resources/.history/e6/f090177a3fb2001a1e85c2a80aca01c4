package Inheritance;

public class test2 extends test_1 {

	public int add(int a, int b) {
		int c = a + b + 20;
		System.out.println(c);
		return c;

	}
	void display() {
		System.out.println(super.sub(12, 20));
	}

	public static void main(String[] args) {
		test2 obj=new test2();
				obj.add(10, 1);
				obj.display();

	}

}
